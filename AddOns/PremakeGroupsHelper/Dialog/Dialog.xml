            <Frame parentKey="Difficulty" inherits="PremakeGroupsHelperDropdownTemplate" hidden="false">
                <Anchors>
                    <Anchor point="TOPLEFT" x="9" y="-47"/>
                </Anchors>
            </Frame>
            <Frame parentKey="Dungeon" inherits="PremakeGroupsHelperDropdownTemplate" hidden="false">
