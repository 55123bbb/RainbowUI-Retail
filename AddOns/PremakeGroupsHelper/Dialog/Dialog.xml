<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <!-- basic template for a filter row: a checkbox and a title -->
    <Frame name="PremakeGroupsHelperBasicTemplate" virtual="true">
        <Size x="280" y="23"/>
        <Frames>
            <CheckButton parentKey="Act" inherits="UICheckButtonTemplate" checked="false">
                <Anchors>
                    <Anchor point="TOPLEFT"/>
                </Anchors>
            </CheckButton>
        </Frames>
        <Layers>
            <Layer>
                <FontString parentKey="Title" inherits="GameFontHighlight" wordwrap="false" justifyH="LEFT">
                    <Size x="130" y="17"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="35" y="-6"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>

    <!-- filter row with a dropdown box after the checkbox and the title -->
    <Frame name="PremakeGroupsHelperDropdownTemplate" inherits="PremakeGroupsHelperBasicTemplate" virtual="true">
        <Frames>
            <Frame parentKey="DropDown" inherits="UIDropDownMenuTemplate">
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-101"/>
                </Anchors>
            </Frame>
        </Frames>
    </Frame>

    <!-- filter row with minimum and maximum input box after the checkbox and the title -->
    <Frame name="PremakeGroupsHelperMinMaxTemplate" inherits="PremakeGroupsHelperBasicTemplate" virtual="true">
        <Frames>
            <EditBox parentKey="Min" inherits="InputBoxTemplate" autoFocus="false" letters="4" numeric="true">
                <Size x="45" y="20"/>
                <Anchors>
                    <Anchor point="TOPLEFT" x="170" y="-4"/>
                </Anchors>
            </EditBox>
            <EditBox parentKey="Max" inherits="InputBoxTemplate" autoFocus="false" letters="4" numeric="true">
                <Size x="45" y="20"/>
                <Anchors>
                    <Anchor point="TOPLEFT" x="240" y="-4"/>
                </Anchors>
            </EditBox>
        </Frames>
        <Layers>
            <Layer>
                <FontString parentKey="To" inherits="GameFontHighlight" wordwrap="false">
                    <Size x="25" y="20"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="214" y="-6"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Frame>

    <Button frameStrata="TOOLTIP" name="PremakeGroupsHelperReportButton" inherits="UIPanelCloseButton" hidden="false">
        <!--<Scripts>
            <OnLoad>PremakeGroupsHelper:onBanButtonLoaded(self)</OnLoad>
        </Scripts>-->
    </Button>

	<Button name="PremakeGroupsHelperIgnoreButtonTemplate" virtual="true">
		<Size x="169" y="22"/>
		<Anchors>
			<Anchor point="LEFT">
				<Offset x="2" y="0"/>
			</Anchor>
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="$parentBgTop" inherits="Char-Stat-Top" parentKey="BgTop">
					<Anchors>
						<Anchor point="TOPLEFT" x="0" y="1"/>
					</Anchors>
				</Texture>
				<Texture name="$parentBgBottom" inherits="Char-Stat-Top" parentKey="BgBottom">
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="0" y="-4"/>
					</Anchors>
				</Texture>
				<Texture name="$parentBgMiddle" file="Interface\CharacterFrame\Char-Stat-Middle" parentKey="BgMiddle" vertTile="true">
					<Size x="169" y="8"/>
					<Anchors>
						<Anchor point="LEFT" x="1"/>
					</Anchors>
					<TexCoords left="0.00390625" right="0.66406250" top="0.00000000" bottom="1.00000000"/>
				</Texture>
			</Layer>
			<Layer level="BACKGROUND" textureSubLevel="1">
				<Texture name="$parentStripe" parentKey="Stripe">
					<Anchors>
						<Anchor point="TOPLEFT" x="1" y="0"/>
						<Anchor point="BOTTOMRIGHT" x="0" y="0"/>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="BORDER">
				<Texture name="$parentCheck" file="Interface\Buttons\UI-CheckBox-Check" parentKey="Check" hidden="true">
					<Size x="16" y="16"/>
					<Anchors>
						<Anchor point="LEFT">
							<Offset x="8" y="0"/>
						</Anchor>
					</Anchors>
				</Texture>
			</Layer>
			<Layer level="OVERLAY">
				<Texture name="$parentSelectedBar" parentKey="SelectedBar" file="Interface\FriendsFrame\UI-FriendsFrame-HighlightBar" alpha="0.4" alphaMode="ADD" hidden="true"/>
			</Layer>
		</Layers>
        <Frames>
            <Button name="$parentClean" inherits="UIPanelCloseButton" justifyH="LEFT" parentKey="clean">
                <Anchors>
                    <Anchor point="RIGHT" x="-3" y="0"/>
                </Anchors>
                <!--<OnClick>print("onClearClick")</OnClick>-->
            </Button>
        </Frames>
		<Scripts>
			<OnLoad>
				SetClampedTextureRotation(self.BgBottom, 180);
			</OnLoad>
			<!--<OnClick>
                print("onClick" .. self.text:GetText())
			</OnClick>-->
		</Scripts>
		<ButtonText name="$parentTitleText" inherits="GameFontNormalSmallLeft" justifyH="LEFT" parentKey="text">
			<Anchors>
				<Anchor point="LEFT" relativeTo="$parentCheck" relativePoint="RIGHT" x="-13" y="0"/>
				<Anchor point="RIGHT" x="-3" y="0"/>
			</Anchors>
		</ButtonText>
		<HighlightTexture file="Interface\FriendsFrame\UI-FriendsFrame-HighlightBar-Blue" alphaMode="ADD"/>
	</Button>

    <Frame name="PremakeGroupsHelperIgnoreDialog" parent="UIParent" movable="true" enableMouse="true" inherits="BasicFrameTemplateWithInset" hidden="true" toplevel="true">
        <Size x="217" y="359"/>
        <Anchors>
            <Anchor point="TOPLEFT"/>
        </Anchors>
        <Layers>
            <Layer level="ARTWORK">
                <!-- the text in the title bar -->
                <FontString parentKey="Title" inherits="GameFontNormal" wordwrap="false">
                    <Anchors>
                        <Anchor point="LEFT" x="10"/>
                        <Anchor point="RIGHT" x="-10"/>
                        <Anchor point="TOP" y="-5"/>
                    </Anchors>
                </FontString>
                <!-- advanced -->
                <FontString parentKey="Total" inherits="GameFontNormal" justifyH="LEFT">
                    <Size y="30"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="18" y="-301"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <EditBox parentKey="search" inherits="InputBoxTemplate" autoFocus="false">
                <Size x="180" y="20"/>
                <Anchors>
                    <Anchor point="TOPLEFT" x="18" y="-30"/>
                </Anchors>
            </EditBox>
            <ScrollFrame name="PremakeGroupsHelperIgnoreListPanel" parentKey="lists" inherits="HybridScrollFrameTemplate">
                <Size x="172" y="244"/>
                <Anchors>
                    <Anchor point="TOPLEFT" relativeKey="$parent.search" relativePoint="BOTTOMLEFT" x="-13" y="-10"/>
                </Anchors>
                <Frames>
                    <Slider name="$parentScrollBar" inherits="HybridScrollBarTemplate" parentKey="scrollBar">
                        <Anchors>
                            <Anchor point="TOPLEFT" relativePoint="TOPRIGHT">
                                <Offset x="4" y="-13"/>
                            </Anchor>
                            <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT">
                                <Offset x="4" y="14"/>
                            </Anchor>
                        </Anchors>
                    </Slider>
                </Frames>
            </ScrollFrame>
            <Button parentKey="clearButton" inherits="MagicButtonTemplate">
                <Size x="135" y="22"/>
                <Anchors>
                    <Anchor point="BOTTOMLEFT" x="41" y="12"/>
                </Anchors>
            </Button>
        </Frames>
        <!--<Scripts>
            <OnLoad>PremakeGroupsHelper:IGNORE_DIALOG_LOADED(self)</OnLoad>
        </Scripts>-->
    </Frame>

    <Frame name="PremakeGroupsHelperDialog" parent="UIParent" movable="true" enableMouse="true" inherits="BasicFrameTemplateWithInset" hidden="true" toplevel="true">
        <Size x="300" y="428"/>
        <Anchors>
            <Anchor point="TOPLEFT"/>
        </Anchors>
        <Layers>
            <Layer level="ARTWORK">
                <!-- the text in the title bar -->
                <FontString parentKey="Title" inherits="GameFontNormal" wordwrap="false">
                    <Anchors>
                        <Anchor point="LEFT" x="10"/>
                        <Anchor point="RIGHT" x="-10"/>
                        <Anchor point="TOP" y="-5"/>
                    </Anchors>
                </FontString>
                <!-- text explaining the simple checkbox filters -->
                <FontString parentKey="SimpleExplanation" inherits="GameFontNormal" justifyH="LEFT">
                    <Size y="30"/>
                    <Anchors>
                        <Anchor point="LEFT" x="14"/>
                        <Anchor point="RIGHT" x="-10"/>
                        <Anchor point="TOP" y="-22"/>
                    </Anchors>
                </FontString>
                <!-- text explaining that members etc are the current state of the group -->
                <FontString parentKey="StateExplanation" inherits="GameFontNormal" justifyH="LEFT">
                    <Size x="180" y="30"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="14" y="-160"/>
                    </Anchors>
                </FontString>
                <!-- min -->
                <FontString parentKey="MinExplanation" inherits="GameFontNormal" justifyH="CENTER">
                    <Size x="60" y="30"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="170" y="-160"/>
                    </Anchors>
                </FontString>
                <!-- max -->
                <FontString parentKey="MaxExplanation" inherits="GameFontNormal" justifyH="CENTER">
                    <Size x="60" y="30"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="239" y="-160"/>
                    </Anchors>
                </FontString>
                <!-- advanced -->
                <FontString parentKey="AdvancedExplanation" inherits="GameFontNormal" justifyH="LEFT">
                    <Size y="30"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="14" y="-299"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <CheckButton parentKey="MoveableToggle" setAllPoints="true" checked="false">
                <Anchors>
                    <Anchor point="TOP" x="0" y="5"/>
                    <Anchor point="LEFT" x="-5" y="0"/>
                </Anchors>
                <Size x="32" y="32"/>
                <HitRectInsets left="5" right="5" top="5" bottom="5"/>
                <NormalTexture file="Interface\Buttons\LockButton-Locked-Up"/>
                <!--<PushedTexture file="Interface\Buttons\LockButton-Unlocked-Down"/>-->
                <HighlightTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Highlight" alphaMode="ADD"/>
                <CheckedTexture file="Interface\Buttons\LockButton-Unlocked-Up"/>
            </CheckButton>
            <Button parentKey="IngoreListToggle" setAllPoints="true" hidden="false">
                <Anchors>
                    <Anchor point="TOP" x="0" y="1"/>
                    <Anchor point="RIGHT" x="-26" y="0"/> 
                </Anchors>
                <Size x="22" y="22"/>
                <!--<HitRectInsets left="5" right="5" top="5" bottom="5"/>-->
                <NormalTexture atlas="Campaign-QuestLog-LoreBook">
                    <!--<TexCoords left="=0.283203125" right="0.318359375" top="0.18359375" bottom="0.244140625"/>-->
                </NormalTexture>
                <!--<PushedTexture file="Interface\QuestFrame\QuestMapLogCampaignLore">
                    <TexCoords left="0.5" right="0.890625" top="0.0" bottom="0.78125"/>
                </PushedTexture>-->
                <HighlightTexture alphaMode="ADD" atlas="Campaign-QuestLog-LoreBook-Highlight"/>
            </Button>
